<main class="content">
    <div class="d-flex justify-content-center align-items-center">
        <a href="../<%= year %>">
            <button class="btn btn-secondary gallery" onclick="window.history.back()">Main Gallery</button>
        </a>
            <button class="btn btn-secondary gallery">Community Gallery</button>
    </div>

    <div class="user-images  d-flex flex-wrap justify-content-around">
        <% images.forEach(function(image) { %>
            <% image.imagesLinks.forEach(function(link) { %>
                <div class="p-1">
                    <img class="gallery-image" src="<%= link %>" alt="communityImage.jpg">
                </div>
            <% }); %>
        <% }); %>
    </div>

</main>

